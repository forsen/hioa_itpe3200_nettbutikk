<!DOCTYPE html>
<html>
<head>
    <title>Default</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Custom/SideMenu.css" rel="stylesheet" />
    <link href="~/Content/Custom/MainSheet.css" rel="stylesheet" />
    <script src="http://code.jquery.com/jquery-latest.min.js"
            type="text/javascript"></script>
    <script src="http://code.jquery.com/jquery-latest.js"
            type="text/javascript"></script>
    <script src="~/Scripts/Custom/Collapse.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <link href="~/Content/jquery-ui.theme.min.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.min.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.structure.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.structure.min.css" rel="stylesheet" />
    <link href="~/Content/jquery-ui.theme.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-ui.js"></script>
    <script src="~/Scripts/jquery-ui.min.js"></script>
    <script src="~/Scripts/Custom/Login.js"></script>

    <script type="text/javascript">
        $(function () {
            $("#txtSearch").autocomplete({
                source: '@Url.Action("getResults", "Product")',
                minLength: 1,
                select: function(event, ui)
                {
                    $("#txtSearch").submit();
                }
            });
        });
    </script>







            
   

</head>
<body>
    <div class="container">
        <div class="row">
            <div class="navbar navbar-inverse" role="navigation">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    @Html.ActionLink("Alkohol Mekka", "Frontpage", "Main", new { tom = "" }, new { @class = "navbar-brand" })
                   
                </div>
                <div class="collapse navbar-collapse navbar-ex1-collapse">
                    <div class="col-sm-5 col-md-5">
                        @using (Html.BeginForm("Search", "Product", FormMethod.Get, new { @class = "form-inline", @role = "form" }))
                                {
                                   <div class="form-group">
                                    <div class="input-group">
                                            
                                       @Html.TextBox("searchString", null, new { id = "txtSearch", @class = "form-control", @style = "margin-top:12px", placeholder = "Søk etter vare..." })
                                        </div>
                                       </div>
                            <button class="btn btn-success" type="submit">
                                <span class="glyphicon glyphicon-search"></span>
                                        </button>
                                 
                                }
                    </div>
                    <ul class="nav navbar-nav navbar-right">
                        @if (Session["loggedInUser"] == null)
                        {
                            <li>@Html.ActionLink("Registrer", "Register", "Customer")</li>
                            <li><a href="#LoginModal" id="loginBtn" data-login="true" data-toggle="modal">Logg Inn</a></li>
                            //<li>@Html.ActionLink("Logg Inn", "LogIn", "Customer")</li>
                        }
                        else
                        {
                            <li>@Html.ActionLink("Handlekurv", "viewShoppingCart", "ShoppingCart")</li>
                            <li>@Html.ActionLink("MinSide", "PersonalSite", "Customer")</li>
                            <li>@Html.ActionLink("Logg ut", "LogOut", "Customer")</li>
                        }
                    </ul>
                </div>
                </div>
            </div>
        </div>


    <div class="panel panel-default pull-left">
        <div class="panel-heading">
            <h3>Kategorier</h3>
                        </div>
        <div id="sidebar" class="list-group">
            <a href='@Url.Action("ListProducts", "Product", new { id = 1 })' class="list-group-item" data-parent="#sidebar">
                <i class="icon-group"></i> Øl
                <span class="badge bg_danger"></span>
            </a>
            
            <div id="users" class="list-group subitem collapse">
                <a href="#" class="list-group-item">
                    <i class="icon-caret-right"></i>Overgjæret
                    <span class="badge bg_danger"></span>
                </a>
                <a href="#" class="list-group-item">
                    <i class="icon-caret-right"></i> Undergjøret
                </a>
                <a href="#" class="list-group-item">
                    <i class="icon-caret-right"></i> Spontangjæret
                </a>
                    </div>
            <div id="sidebar" class="list-group">
                <a href="#users" class="list-group-item" data-parent="#sidebar">
                    <i class="icon-group"></i> Rødvin
                    <span class="badge bg_danger"></span>
                </a>
                <div id="users" class="list-group subitem collapse">
                    <a href="#" class="list-group-item">
                        <i class="icon-caret-right"></i>Drikkevin
                        <span class="badge bg_danger"></span>
                    </a>
                    <a href="#" class="list-group-item">
                        <i class="icon-caret-right"></i> Matvin
                    </a>
                    <a href="#" class="list-group-item">
                        <i class="icon-caret-right"></i> For fylla
                    </a>
                        </div>
                <div id="sidebar" class="list-group">
                    <a href="#users" class="list-group-item" data-parent="#sidebar">
                        <i class="icon-group"></i> Hvitvin
                        <span class="badge bg_danger"></span>
                    </a>
                    <div id="users" class="list-group subitem collapse">
                        <a href="#" class="list-group-item">
                            <i class="icon-caret-right"></i>Tørr
                            <span class="badge bg_danger"></span>
                        </a>
                        <a href="#" class="list-group-item">
                            <i class="icon-caret-right"></i> Søt
                        </a>
                    </div>
                    <div id="sidebar" class="list-group">
                        <a href="#users" class="list-group-item" data-parent="#sidebar">
                            <i class="icon-group"></i> Brennevin
                            <span class="badge bg_danger"></span>
                        </a>
                        <div id="users" class="list-group subitem collapse">
                            <a href="#" class="list-group-item">
                                <i class="icon-caret-right"></i>Vodka
                                <span class="badge bg_danger"></span>
                            </a>
                            <a href="#" class="list-group-item">
                                <i class="icon-caret-right"></i> Cognac
                            </a>
                            <a href="#" class="list-group-item">
                                <i class="icon-caret-right"></i> Wiskey
                            </a>
                            <a href="#" class="list-group-item">
                                <i class="icon-caret-right"></i> Rom
                            </a>
                            <a href="#" class="list-group-item">
                                <i class="icon-caret-right"></i> Likør
                            </a>
                            <a href="#" class="list-group-item">
                                <i class="icon-caret-right"></i> Likør over 22%
                            </a>
                            <a href="#" class="list-group-item">
                                <i class="icon-caret-right"></i> Akevitt
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container body-content">
            
        @RenderBody()
            <hr />
        <footer>
            <p>&copy;@DateTime.Now.Year - Gruppe 12345</p>
        </footer>
    </div>
    @Html.Partial("~/Views/Customer/LoginModal.cshtml")
</body>

</html>


